target 'BooksArchiveDemo' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for BooksArchiveDemo
	pod 'Alamofire'
	pod 'AlamofireImage'

  target 'BooksArchiveDemoTests' do
    inherit! :search_paths
    # Pods for testing
	pod 'Alamofire'
	pod 'AlamofireImage'
  end

  target 'BooksArchiveDemoUITests' do
    inherit! :search_paths
    # Pods for testing
	pod 'Alamofire'
	pod 'AlamofireImage'
  end

end
